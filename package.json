{
	"name": "code-snapshot",
	"displayName": "Code Snapshot",
	"description": "Create your custom code snippet",
	"version": "0.0.1",
	"publisher": "robertz",
	"engines": {
		"vscode": "^1.41.0"
	},
	"icon": "icon.png",
	"categories": [
		"Other"
	],
	"keywords": [
		"snapshot",
		"snippet",
		"code",
		"screenshot",
		"javascript",
		"js",
		"jsx",
		"flow",
		"typescript",
		"ts",
		"json",
		"css",
		"less",
		"scss",
		"styled-components",
		"styled-jsx",
		"markdown",
		"md",
		"commonmark",
		"mdx",
		"php",
		"pug",
		"ruby",
		"swift",
		"html",
		"vue",
		"angular",
		"graphql",
		"yaml",
		"yml"
	],
	"repository": {
		"type": "git",
	  	"url": "https://github.com/robert-z/code-snapshot"
	},
	"bugs": {
		"url": "https://github.com/robert-z/code-snapshot/issues"
	},
	"activationEvents": [
		"onCommand:codesnapshot.init"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "codesnapshot.init",
				"title": "Code Snapshot"
			}
		],
		"menus": {
		  "editor/context": [
			{
			  "command": "codesnapshot.init"
			}
		  ]
		}
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/glob": "^7.1.1",
		"@types/mocha": "^5.2.7",
		"@types/node": "^12.11.7",
		"@types/vscode": "^1.41.0",
		"glob": "^7.1.5",
		"mocha": "^6.2.2",
		"typescript": "^3.6.4",
		"tslint": "^5.20.0",
		"vscode-test": "^1.2.2"
	}
}
